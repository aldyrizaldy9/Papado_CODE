<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aldy.papado.PenyediaMainv2Activity"
    android:id="@+id/penyedia_drawer_layout"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include layout="@layout/penyedia_nav_action"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Penyedia main v2"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:textSize="24sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="asdfasdfsadf"/>
        <ListView
            android:id="@+id/penyedia_list_lapangan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

        </ListView>
    </LinearLayout>
    <android.support.design.widget.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:menu="@menu/penyedia_nav_menu"
        android:layout_gravity="start"
        app:headerLayout="@layout/penyedia_nav_header"
        android:id="@+id/penyedia_nav_view">

    </android.support.design.widget.NavigationView>
</android.support.v4.widget.DrawerLayout>
